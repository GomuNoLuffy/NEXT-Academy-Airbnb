
<%= form_for [@listing, @booking] do |f| %>

	<div class="text-field">
  	<%= f.label :start_date %><br>
  	<%= f.date_field :start_date %><br>
  	<%= f.label :end_date %><br>
  	<%= f.date_field :end_date %><br>
    <%= f.label :number_of_guest %><br>
    <%= f.number_field :num_guest %><br>
  	<%= f.submit %>
  </div>
<% end %>

<%= @errors %>
